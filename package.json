    "check-types": "turbo run check-types",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "devDependencies": {
    "prettier": "^3.5.0",
    "turbo": "^2.4.2",
    "typescript": "5.7.3"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "bun@1.2.2",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "react-native-reanimated": "^3.16.7",
    "expo": "~52.0.37",
    "react": "18.3.1",
    "react-native": "0.76.7"
  },
  "version": "1.0.0"
}
